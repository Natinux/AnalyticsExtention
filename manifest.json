{
    "name": "Tab-Auto-Switcher",
    "version": "0.0.2",
    "manifest_version": 2,
    "description":"Tabs Auto Switcher.",
    "icons":{
        "16": "icons/16x16.png",
        "19": "icons/19x19.png",
        "38": "icons/38x38.png",
        "48": "icons/48x48.png",
        "128": "icons/128x128.png"
    },
    "options_page": "html/options.html",
    "browser_action":{
    	"default_icon":{
    		"19": "icons/19x19.png",
    		"38": "icons/38x38.png"
    	},
    	"default_title": "Tab-Auto-Switcher",
    	"default_popup": "html/popup.html"
    },
    "background":{
    	"scripts": ["js/background.js"],
    	"persistent": true
    },
    "permissions":[
        "tabs",
        "*://*/*"
    ]
}